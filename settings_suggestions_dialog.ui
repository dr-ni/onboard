                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="receives_default">False</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip_markup" translatable="yes">Add and remove spaces when inserting word suggestions followed by punctuation characters.</property>
                        <property name="tooltip_text" translatable="yes">Add and remove spaces when inserting word suggestions followed by punctuation characters.</property>
