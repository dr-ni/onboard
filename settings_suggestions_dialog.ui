    <action-widgets>
      <action-widget response="0">dialog_close_button</action-widget>
    </action-widgets>
  </object>
  <object class="GtkListStore" id="spell_check_backend_model">
    <columns>
      <!-- column-name Backend -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0">hunspell</col>
      </row>
      <row>
        <col id="0">aspell</col>
      </row>
    </data>
  </object>
</interface>
